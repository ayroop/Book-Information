{
    "name": "pooriya/book-information",
    "type": "wordpress-plugin",
    "require": {
        "php": "^7.4",
        "veronalabs/rabbit": "^1.4.5",
        "illuminate/database": "^8.61",
        "caseyamcl/configula": "^4.3",
        "league/container": "^3.3",
        "laminas/laminas-escaper": "^2.7",
        "veronalabs/logger-wp": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BookInformation\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}